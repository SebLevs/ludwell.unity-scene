#search-field {
    background-color: rgba(0, 0, 0, 0);
    align-self: center;
    justify-content: center;
    align-items: center;
    padding-left: 0;
    padding-right: 0;
    padding-top: 0;
    padding-bottom: 0;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

#unity-search:hover {
    transition-duration: 0.2s;
    transition-timing-function: ease;
    scale: 1.5 1.4;
}
